datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

generator client {
  provider = "prisma-client-js"
}

model Agenda {
  id        Int      @id @default(autoincrement())
  titulo    String
  data      DateTime
  local     String
  createdAt DateTime @default(now())
}

model Noticia {
  id        Int      @id @default(autoincrement())
  titulo    String
  conteudo  String
  createdAt DateTime @default(now())
}
